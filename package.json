{
  "name": "jewellary",
  "version": "1.0.0",
  "description": "Jewelry store backend using Node.js, Express, and Prisma",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/app.js",
    "start": "node src/app.js",
