    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.14.0",
    "base64-arraybuffer": "^1.0.2",
    "prisma": "^6.5.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=16.0.0"
